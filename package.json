{
  "name": "miqdaddev",
  "version": "2.2.0",
  "description": "Miqdad Personal Website",
  "scripts": {
    "clean": "rm -rf ./dist",
    "build:assets": "webpack --mode production",
    "build:site": "eleventy",
    "build": "run-s clean build:*",
    "dev:assets": "cross-env NODE_ENV=development webpack --mode development --watch",
    "dev:site": "cross-env NODE_ENV=development eleventy --serve",
    "dev": "npm run clean && run-p dev:*",
    "precommit": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/miqdadfwz/miqdad.dev.git"
  },
  "author": {
    "name": "Miqdad Abdurrahman Fawwaz",
    "email": "miqdad.fawwaz@gmail.com",
    "url": "https://miqdad.dev"
  },
  "license": "GPL-3.0-only",
  "bugs": {
    "url": "https://github.com/miqdadfwz/miqdad.dev/issues"
  },
  "homepage": "https://github.com/miqdadfwz/miqdad.dev#readme",
  "devDependencies": {
    "@11ty/eleventy": "2.0.1",
    "@commitlint/cli": "19.3.0",
    "@commitlint/config-conventional": "19.2.2",
    "@tailwindcss/typography": "0.5.15",
    "@types/node": "20.16.5",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "autoprefixer": "10.4.20",
    "cross-env": "7.0.3",
    "css-loader": "7.1.2",
    "cssnano": "7.0.6",
    "cz-conventional-changelog": "3.3.0",
    "eleventy-google-fonts": "0.1.0",
    "eleventy-plugin-seo": "0.5.2",
    "esbuild-loader": "2.21.0",
    "eslint": "9.7.0",
    "eslint-config-google": "0.14.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "husky": "9.1.5",
    "lint-staged": "15.2.7",
    "luxon": "3.4.4",
    "mini-css-extract-plugin": "2.9.0",
    "npm-run-all2": "5.0.2",
    "postcss": "8.4.39",
    "postcss-loader": "7.3.4",
    "prettier": "2.8.8",
    "tailwindcss": "3.4.6",
    "typescript": "5.5.3",
    "webpack": "5.93.0",
    "webpack-cli": "5.1.4",
    "webpack-manifest-plugin": "5.0.0",
    "webpackbar": "5.0.2"
  },
  "dependencies": {
    "workbox-cacheable-response": "6.6.1",
    "workbox-expiration": "6.6.0",
    "workbox-navigation-preload": "6.6.0",
    "workbox-precaching": "6.6.0",
    "workbox-routing": "6.6.0",
    "workbox-strategies": "6.6.0",
    "workbox-webpack-plugin": "6.6.0"
  },
  "lint-staged": {
    "*.{css,html,json,js,ts}": [
      "prettier --write"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
