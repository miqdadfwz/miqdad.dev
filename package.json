{
  "name": "miqdaddev",
  "version": "2.2.0",
  "description": "Miqdad Personal Website",
  "scripts": {
    "clean": "rm -rf ./dist",
    "build:assets": "webpack --mode production",
    "build:site": "eleventy",
    "build": "run-s clean build:*",
    "dev:assets": "cross-env NODE_ENV=development webpack --mode development --watch",
    "dev:site": "cross-env NODE_ENV=development eleventy --serve",
    "dev": "npm run clean && run-p dev:*",
    "precommit": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/miqdadfwz/miqdad.dev.git"
  },
  "author": {
    "name": "Miqdad Abdurrahman Fawwaz",
    "email": "miqdad.fawwaz@gmail.com",
    "url": "https://miqdad.dev"
  },
  "license": "GPL-3.0-only",
  "bugs": {
    "url": "https://github.com/miqdadfwz/miqdad.dev/issues"
  },
  "homepage": "https://github.com/miqdadfwz/miqdad.dev#readme",
  "devDependencies": {
    "@11ty/eleventy": "1.0.1",
    "@commitlint/cli": "17.0.3",
    "@commitlint/config-conventional": "17.0.3",
    "@tailwindcss/typography": "0.5.4",
    "@types/node": "15.0.1",
    "@typescript-eslint/eslint-plugin": "5.30.7",
    "@typescript-eslint/parser": "5.30.7",
    "autoprefixer": "10.4.7",
    "cross-env": "7.0.3",
    "css-loader": "6.7.1",
    "cssnano": "5.1.12",
    "cz-conventional-changelog": "3.3.0",
    "eleventy-google-fonts": "0.0.5",
    "eleventy-plugin-seo": "0.5.2",
    "esbuild-loader": "2.19.0",
    "eslint": "8.20.0",
    "eslint-config-google": "0.14.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "luxon": "3.0.1",
    "mini-css-extract-plugin": "2.6.1",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.14",
    "postcss-loader": "7.0.1",
    "prettier": "2.7.1",
    "tailwindcss": "3.1.6",
    "typescript": "4.7.4",
    "webpack": "5.73.0",
    "webpack-cli": "4.10.0",
    "webpack-manifest-plugin": "5.0.0",
    "webpackbar": "5.0.2"
  },
  "dependencies": {
    "workbox-cacheable-response": "6.5.0",
    "workbox-expiration": "6.5.0",
    "workbox-navigation-preload": "6.5.0",
    "workbox-precaching": "6.5.0",
    "workbox-routing": "6.5.0",
    "workbox-strategies": "6.5.0",
    "workbox-webpack-plugin": "6.5.0"
  },
  "lint-staged": {
    "*.{css,html,json,js,ts}": [
      "prettier --write"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
