{
  "name": "miqdaddev",
  "version": "0.0.1",
  "description": "Miqdad Personal Website",
  "main": "index.js",
  "scripts": {
    "dev:assets": "cross-env NODE_ENV=development webpack --mode development --watch",
    "dev:site": "cross-env NODE_ENV=development eleventy --serve",
    "dev": "pnpm run clean && run-p dev:*",
    "build": "run-s clean build:*",
    "build:assets": "webpack --mode production",
    "build:site": "eleventy",
    "clean": "rm -rf ./dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/miqdadfwz/miqdad.dev.git"
  },
  "keywords": [
    "miqdad"
  ],
  "author": "Miqdad Abdurrahman Fawwaz",
  "license": "GPL-3.0-only",
  "engines": {
    "node": ">=12",
    "npm": ">=6"
  },
  "bugs": {
    "url": "https://github.com/miqdadfwz/miqdad.dev/issues"
  },
  "homepage": "https://github.com/miqdadfwz/miqdad.dev#readme",
  "devDependencies": {
    "@11ty/eleventy": "0.12.1",
    "@11ty/eleventy-navigation": "0.2.0",
    "@11ty/eleventy-plugin-rss": "1.1.1",
    "@babel/core": "7.14.0",
    "@babel/preset-env": "7.14.0",
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@fullhuman/postcss-purgecss": "4.0.3",
    "@types/node": "15.0.1",
    "@typescript-eslint/eslint-plugin": "4.22.0",
    "@typescript-eslint/parser": "4.22.0",
    "autoprefixer": "10.2.5",
    "cross-env": "7.0.3",
    "css-loader": "5.2.4",
    "cssnano": "5.0.2",
    "cz-conventional-changelog": "3.3.0",
    "eleventy-google-fonts": "0.0.5",
    "eleventy-plugin-lazyimages": "2.1.0",
    "esbuild-loader": "2.12.0",
    "eslint": "7.25.0",
    "eslint-config-google": "0.14.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-prettier": "3.4.0",
    "husky": "6.0.0",
    "lint-staged": "10.5.4",
    "mini-css-extract-plugin": "1.6.0",
    "npm-run-all": "4.1.5",
    "postcss": "8.2.13",
    "postcss-loader": "5.2.0",
    "prettier": "2.2.1",
    "tailwindcss": "2.1.2",
    "webpack": "5.36.2",
    "webpack-cli": "4.6.0",
    "webpack-manifest-plugin": "3.1.1",
    "webpackbar": "5.0.0-3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "lint-staged": {
    "*.{css,html,json,jsx,js,ts,tsx}": [
      "prettier --write"
    ],
    "*.{jsx, js, ts, tsx}": [
      "tsc --noEmit && eslint"
    ]
  },
  "dependencies": {
    "workbox-cacheable-response": "^6.1.5",
    "workbox-expiration": "^6.1.5",
    "workbox-precaching": "^6.1.5",
    "workbox-routing": "^6.1.5",
    "workbox-strategies": "^6.1.5",
    "workbox-webpack-plugin": "^6.1.5"
  }
}
