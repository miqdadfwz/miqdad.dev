{
  "name": "miqdaddev",
  "version": "1.1.4",
  "description": "Miqdad Personal Website",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf ./dist",
    "build:assets": "webpack --mode production",
    "build:site": "eleventy",
    "prebuild": "test \"$CI\" = true && npx pnpm install -r --store=node_modules/.pnpm-store || echo skipping pnpm install",
    "build": "run-s clean build:*",
    "dev:assets": "cross-env NODE_ENV=development webpack --mode development --watch",
    "dev:site": "cross-env NODE_ENV=development eleventy --serve",
    "dev": "pnpm run clean && run-p dev:*",
    "prepare": "husky install",
    "pre-commit": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/miqdadfwz/miqdad.dev.git"
  },
  "keywords": [
    "miqdad"
  ],
  "author": "Miqdad Abdurrahman Fawwaz",
  "license": "GPL-3.0-only",
  "engines": {
    "node": ">=12",
    "npm": ">=6"
  },
  "bugs": {
    "url": "https://github.com/miqdadfwz/miqdad.dev/issues"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "homepage": "https://github.com/miqdadfwz/miqdad.dev#readme",
  "devDependencies": {
    "@11ty/eleventy": "0.12.1",
    "@11ty/eleventy-navigation": "0.2.0",
    "@11ty/eleventy-plugin-rss": "1.1.1",
    "@11ty/eleventy-plugin-syntaxhighlight": "3.1.2",
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@tailwindcss/typography": "0.4.1",
    "@types/node": "15.0.1",
    "@typescript-eslint/eslint-plugin": "4.29.1",
    "@typescript-eslint/parser": "4.29.1",
    "cz-conventional-changelog": "3.3.0",
    "eleventy-google-fonts": "0.0.5",
    "eleventy-plugin-seo": "0.5.2",
    "eleventy-plugin-time-to-read": "1.0.0",
    "eslint": "7.27.0",
    "eslint-config-google": "0.14.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.4.0",
    "husky": "7.0.1",
    "lint-staged": "11.0.0",
    "luxon": "1.28.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.3.0",
    "tailwindcss": "2.2.7",
    "typescript": "4.2.4"
  },
  "lint-staged": {
    "*.{css,html,json,js,ts}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@fullhuman/postcss-purgecss": "4.0.3",
    "autoprefixer": "10.2.5",
    "cross-env": "7.0.3",
    "css-loader": "5.2.6",
    "cssnano": "5.0.7",
    "esbuild-loader": "2.13.1",
    "markdown-it": "12.0.6",
    "markdown-it-anchor": "7.1.0",
    "mini-css-extract-plugin": "1.6.0",
    "postcss": "8.3.6",
    "postcss-loader": "5.3.0",
    "webpack": "5.50.0",
    "webpack-cli": "4.7.0",
    "webpack-manifest-plugin": "3.1.1",
    "webpackbar": "5.0.0-3",
    "workbox-cacheable-response": "6.1.5",
    "workbox-expiration": "6.1.5",
    "workbox-navigation-preload": "6.1.5",
    "workbox-precaching": "6.1.5",
    "workbox-routing": "6.1.5",
    "workbox-strategies": "6.1.5",
    "workbox-webpack-plugin": "6.1.5"
  }
}
